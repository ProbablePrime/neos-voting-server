{
  "name": "typescript-boilerplate",
  "description": "A boilerplate to help me get started on typescript stuff quickly",
  "author": "ProbablePrime <prime@ahref.co.uk>",
  "license": "MIT",
  "version": "0.1.0",
  "main": "src/index.js",
  "scripts": {
    "fmt": "prettier --write **/*.ts",
    "deps:update": "ncu -ua",
    "start": "node src/index.js"
  },
  "engineStrict": true,
  "engines": {
    "node": ">= 12.0.0"
  },
  "private": true,
  "devDependencies": {
    "npm-check-updates": "^9.0.0",
    "rimraf": "^3.0.2"
  },
  "prettier": {
    "parser": "typescript",
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@keyv/sqlite": "^2.0.2",
    "bluefill": "^0.1.3",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "json2csv": "^5.0.1",
    "keyv": "^4.0.1",
    "local-hostname": "^0.1.0",
    "node-fetch": "^2.6.1",
    "polka": "^0.5.2",
    "simple-node-logger": "^18.12.24"
  }
}
